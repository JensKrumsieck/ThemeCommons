<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>JensKrumsieck.ThemeCommons</PackageId>
    <Authors>Jens Krumsieck</Authors>
    <TargetFramework>net5.0-windows</TargetFramework>
    <LangVersion>9.0</LangVersion>
    <UseWPF>true</UseWPF>
    <Version>1.0.0-rc1</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/JensKrumsieck/ThemeCommons</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JensKrumsieck/ThemeCommons</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="Assets\Montserrat\Montserrat-Black.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-BlackItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-Bold.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-BoldItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-ExtraBold.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-ExtraBoldItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-ExtraLight.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-ExtraLightItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-Italic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-Light.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-LightItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-Medium.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-MediumItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-Regular.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-SemiBold.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-SemiBoldItalic.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-Thin.ttf" />
    <Resource Include="Assets\Montserrat\Montserrat-ThinItalic.ttf" />
    <Resource Include="Assets\noise.png" />
  </ItemGroup>
</Project>
